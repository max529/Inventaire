namespace App.Http.Controllers.Auth.Logout {

	export class AuthLogoutController extends Aventus.HttpRoute {
		@BindThis()
		public async request(): Promise<Aventus.VoidWithError> {
			const request = new Aventus.HttpRequest(`${this.getPrefix()}/logout`, Aventus.HttpMethod.POST);
			return await request.queryVoid(this.router);
		}
	}

}