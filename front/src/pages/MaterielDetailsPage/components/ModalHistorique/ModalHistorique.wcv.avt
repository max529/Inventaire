<div class="title">Historique pour {{ this.variation.nom }} {{ this.getVariations() }} pour {{ this.equipe.nom }}</div>

<div class="content">
    <av-flex-scroll @scrollChange="loadMoreData" @element="scrollEl" floating_scroll>
        for(let historique of this.historiques) {
            <div class="line">
                {{ this.getTxt(historique) }}
            </div>
        }
    </av-flex-scroll>
</div>

<div class="footer">
    <av-button @press="reject">Fermer</av-button>
</div>