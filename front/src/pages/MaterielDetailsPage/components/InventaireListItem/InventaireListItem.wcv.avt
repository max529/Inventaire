<div class="line">
    <div class="nom">{{ this.inventaire.equipe.nom }}</div>
    <div class="variation">{{ this.inventaire.variation?.nom }}</div>
    <div class="quantite">{{ this.inventaire.quantite ?? '-' }}</div>
    <div class="modification">
        <div class="actions">
            <av-icon-action color="neutral" icon="edit" @press="edit">Modifier</av-icon-action>
            if(this.inventaire.id) {
                <av-icon-action color="info" icon="history" @press="historique">Historique</av-icon-action>
            }
        </div>
        <span>{{ this.getLastUpdate() }}</span>
    </div>
</div>